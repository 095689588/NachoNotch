<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <Preference
        android:title="@string/enable_launcher"
        android:key="enable_launcher"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/status_height"
        android:key="status_height"
        app:msbp_minValue="0"
        app:msbp_maxValue="300"
        />

    <SwitchPreference
        android:title="@string/rounded_corners_top"
        android:key="rounded_corners"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/top_corner_width"
        android:key="top_corner_width"
        android:defaultValue="24"
        app:msbp_minValue="0"
        app:msbp_maxValue="56"
        app:msbp_measurementUnit="dp"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/top_corner_height"
        android:key="top_corner_height"
        android:defaultValue="24"
        app:msbp_minValue="0"
        app:msbp_maxValue="56"
        app:msbp_measurementUnit="dp"
        />

    <SwitchPreference
        android:title="@string/cover_nav"
        android:key="cover_nav"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/nav_height"
        android:key="nav_height"
        app:msbp_minValue="0"
        app:msbp_maxValue="400"
        />

    <SwitchPreference
        android:title="@string/rounded_corners_bottom"
        android:key="rounded_corners_bottom"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/bottom_corner_width"
        android:key="bottom_corner_width"
        android:defaultValue="24"
        app:msbp_minValue="0"
        app:msbp_maxValue="56"
        app:msbp_measurementUnit="dp"
        />

    <com.pavelsikun.seekbarpreference.SeekBarPreference
        android:title="@string/bottom_corner_height"
        android:key="bottom_corner_height"
        android:defaultValue="24"
        app:msbp_minValue="0"
        app:msbp_maxValue="56"
        app:msbp_measurementUnit="dp"
        />

</PreferenceScreen>